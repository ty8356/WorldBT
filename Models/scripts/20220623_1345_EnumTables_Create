CREATE TABLE [Histology] (
    [Id] int NOT NULL CONSTRAINT PK_Histology_Id PRIMARY KEY IDENTITY(1, 1)
    ,[Name] nvarchar(30) NULL
)

CREATE TABLE [Subgroup] (
    [Id] int NOT NULL CONSTRAINT PK_Subgroup_Id PRIMARY KEY IDENTITY(1, 1)
    ,[Name] nvarchar(30) NULL
)

CREATE TABLE [Location] (
    [Id] int NOT NULL CONSTRAINT PK_Location_Id PRIMARY KEY IDENTITY(1, 1)
    ,[Name] nvarchar(100) NULL
)

CREATE TABLE [TissueType] (
    [Id] int NOT NULL CONSTRAINT PK_TissueType_Id PRIMARY KEY IDENTITY(1, 1)
    ,[Name] nvarchar(30) NULL
)

